{
  "name": "dllobell/sharpspring-api",
  "description": "Sharpspring API PHP Client",
  "license": "MIT",
  "keywords": [
    "dllobell",
    "sharpspring",
    "api"
  ],
  "authors": [
    {
      "name": "David Llobell",
      "email": "dllobellmoya@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "symfony/var-dumper": "^4.3"
  },
  "autoload": {
    "psr-4": {
      "Dllobell\\SharpspringApi\\": "src"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Dllobell\\SharpspringApi\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
        "providers": [
            "Dllobell\\SharpspringApi\\Laravel\\SharpspringServiceProvider"
        ],
        "aliases": {
            "Sharpspring": "Dllobell\\SharpspringApi\\Laravel\\Facades\\Sharpspring"
        }
    }
},
  "minimum-stability": "dev",
  "prefer-stable": true
}
